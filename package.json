{
  "name": "my-backend-extra",
  "version": "1.0.0",
  "description": "My Backend Extra",
  "author": "M Haidar Hanif",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "dev": "nest start --watch",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "prisma db push && node dist/src/main",
    "build": "prisma generate && nest build",
    "prisma:format": "prisma format",
    "prisma:validate": "prisma validate",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio --port 5555 --browser none",
    "prisma:seed": "prisma db seed",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" --cache",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "tsx prisma/seed.ts"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "dependencies": {
    "@nestjs/cli": "^9.4.2",
    "@nestjs/common": "^9.4.0",
    "@nestjs/core": "^9.4.0",
    "@nestjs/jwt": "10.0.3",
    "@nestjs/passport": "9.0.3",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/swagger": "6.3.0",
    "@prisma/client": "4.14.1",
    "@types/bcrypt": "5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.1",
    "@types/node": "20.1.3",
    "@types/passport-jwt": "3.0.8",
    "@types/supertest": "^2.0.12",
    "bcrypt": "5.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "helmet": "6.0.1",
    "nanoid": "3.3.6",
    "nestjs-prisma": "0.20.0",
    "passport": "0.6.0",
    "passport-jwt": "4.0.1",
    "prisma": "4.14.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "slugify": "1.6.6",
    "swagger-ui-express": "4.6.3",
    "unsplash-js": "7.0.18"
  },
  "devDependencies": {
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^9.4.0",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.5.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "tsx": "3.12.7",
    "typescript": "^4.7.4"
  }
}
